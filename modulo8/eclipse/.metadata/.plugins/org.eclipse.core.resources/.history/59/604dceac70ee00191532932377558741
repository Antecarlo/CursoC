#ifndef FICHERO_H_
#define FICHERO_H_

#include <string>
#include <map>

class Fichero {

	std::string path;
	std::map<std::string,std::string> configuracion;

public:
	Fichero(std::string path);

	void load();
	void save();
	std::string getKey(std::string key);
	void setKey(std::string key, std:string value);


	virtual ~Fichero();
};

#endif /* FICHERO_H_ */
