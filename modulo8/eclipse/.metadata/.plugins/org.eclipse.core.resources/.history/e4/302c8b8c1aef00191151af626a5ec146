#include <iostream>
#include "stdio.h"
#include <string.h>
#include <vector>
#include <fstream>
#include <stdexcept>

#include "Fichero.h"
#include "PruebaIdioma.h"

int main(int argc, char *argv[]){

	if(argc==2){
		std::string end=".ini";
		std::cout << argv[1]+end << std::endl;
		Fichero fichero(argv[1]+end);
		fichero.imprimirMapa();
		PruebaIdioma idioma(argv[1]+"_config"+end);

	}else{
		std::cout << "Few arguments... :"<< argc << std::endl;

	}



}


