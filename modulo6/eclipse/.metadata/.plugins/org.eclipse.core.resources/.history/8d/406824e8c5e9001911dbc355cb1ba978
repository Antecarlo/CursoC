#include "Acertijos.h"
#include <cstdlib>
#include <ctime>


Acertijos::Acertijos() {


}

void Acertijos::generarClave() {
	std::string vocales= "AEIOU";
	std::string consonantes = "ZXVBNMSDFGHJKLQWRTYP";

	std::srand(std::time(0));
	int iniVocal = std::rand()%vocales.length();
	int inCons = std::rand()%consonantes.length();

}

