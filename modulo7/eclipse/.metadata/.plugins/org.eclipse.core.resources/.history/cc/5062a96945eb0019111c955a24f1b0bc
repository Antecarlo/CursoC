#include "Matriz.h"

Matriz::Matriz(int filas, int col, int val) {

	this->filas=filas;
	this->col=col;

	//Primero crear el array de fuera:
	//Hazme [filas] punteros unique de tipo int []
	this->datos=std::make_unique<std::unique_ptr<int []>[]>(this->filas);
	std::make_unique<>(col);
	for(int i=0;i<col;i++)
		this->datos[]
}

