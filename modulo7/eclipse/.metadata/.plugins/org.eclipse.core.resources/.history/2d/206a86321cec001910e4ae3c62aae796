#include "Cuenta.h"
#include <iostream>


Cuenta::Cuenta(Cliente cliente, double saldo): cliente(cliente),saldo(saldo) {
}

void Cuenta::addMovimiento(Movimiento mov) {
	this->movimientos.push_back(mov);
}

void Cuenta::imprimir() {
	std::cout << "Datos de la cuenta" << std::endl;
	std::cout << "Cliente: " << this->cliente << ", saldo: "<< this->saldo
}

Cuenta::~Cuenta() {
	// TODO Auto-generated destructor stub
}

