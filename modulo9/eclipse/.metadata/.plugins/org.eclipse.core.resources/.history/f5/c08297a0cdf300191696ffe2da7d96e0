#include "Coche.h"
#include "Apagado.h"

Coche::Coche(int combustible=100,int velocidad=0,std::shared_ptr<State> inicio): estado(inicio),combustible(combustible), velocidad(velocidad) {
}

void Coche::frenar() {
	this->estado->frenar(*this);
}

void Coche::acelerar() {
	this->estado->acelerar(*this);
}

void Coche::contacto() {
	this->estado->contacto(*this);
}

Coche::~Coche() {
	// TODO Auto-generated destructor stub
}

