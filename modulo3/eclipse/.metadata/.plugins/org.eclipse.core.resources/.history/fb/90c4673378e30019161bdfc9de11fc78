#ifndef FIGURA_H_
#define FIGURA_H_

#include<string>

class Figura {

	std::string etiqueta;
	std::string color;


public:
	Figura(std::string etiqueta="",std::string color="black");

	virtual void dibujar()=0; //Metodo virtual puro


	virtual ~Figura();
};


#endif /* FIGURA_H_ */
