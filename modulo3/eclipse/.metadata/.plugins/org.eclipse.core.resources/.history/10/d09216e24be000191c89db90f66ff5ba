#ifndef HORA_H_
#define HORA_H_

class Hora {

	int h,m,s;

public:
	Hora();
	Hora(int horas, int minutos, int segundos);
	Hora(int horas, int minutos=0, int segundos=0);
	Hora(int horas, int minutos, int segundos=0);

	void imprimir()const;

	virtual ~Hora();
};

#endif /* HORA_H_ */
