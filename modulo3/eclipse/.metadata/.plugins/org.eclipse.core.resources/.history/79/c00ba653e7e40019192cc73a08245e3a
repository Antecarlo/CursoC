#ifndef CLIENTE_H_
#define CLIENTE_H_

#include <iostream>



class Cliente {

	std::string nombre;
	std::string apellidos;
	long numero;

public:
	Cliente();
	Cliente(std::string nombre,std::string apellidos,long numero);
	virtual ~Cliente();
};


#endif /* CLIENTE_H_ */
