#ifndef VECTOR_H_
#define VECTOR_H_

namespace std {
namespace literals {

class Vector {

	int * numeros;
	int size;
	int cont;

public:
	Vector(int * numeros,int tama=10, int cont);

	void imprimir();

	Vector add();

	virtual ~Vector();
};

} /* namespace literals */
} /* namespace std */

#endif /* VECTOR_H_ */
