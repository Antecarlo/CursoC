#ifndef EVENTO_H_
#define EVENTO_H_
#include "Fecha.h"
#include "Hora.h"


class Evento {

protected:

	long numDestino;
	double importe;
	Fecha fecha;
	Hora horaIn;

public:
	Evento();

	Evento(long numD,double importe, Fecha fecha, Hora inicio);

	virtual double calcular()=0;

	virtual ~Evento();
};


#endif /* EVENTO_H_ */
