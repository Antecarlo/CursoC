Esfera::Esfera(std::string etiqueta,std::string color,double ratio):Figura3D(etiqueta,color) {
	this->ratio=ratio;
}

void Esfera::dibujar() {
	std::cout << "Etiqueta:" << this->getEtiqueta() << ", Color: "<< this->getColor()<< ", Radio: "<< this->ratio << std::endl;

}

