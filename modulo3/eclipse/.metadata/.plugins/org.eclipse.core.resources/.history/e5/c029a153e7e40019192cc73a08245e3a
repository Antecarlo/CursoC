#include "Cliente.h"


Cliente::Cliente() {
	this->nombre="anonimo";
	this->apellidos="none";
	this->numero=0;

}

Cliente::Cliente(std::string nombre,std::string apellidos,long numero){
	this->nombre=nombre;
	this->apellidos=apellidos;
	this->numero=numero;
}

Cliente::~Cliente() {
	// TODO Auto-generated destructor stub
}
