#include "FechaHora.h"

friend std::ostream & operator<<(std::ostream & os, const FechaHora & fh){
	return os << fh.f << " "<< fh.h;
}

FechaHora::~FechaHora() {
	// TODO Auto-generated destructor stub
}

FechaHora::FechaHora(Fecha fecha, Hora hora): f(fecha),h(hora) {
}
