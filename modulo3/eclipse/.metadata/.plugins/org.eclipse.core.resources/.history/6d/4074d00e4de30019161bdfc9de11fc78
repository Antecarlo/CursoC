#ifndef EMPLEADO_H_
#define EMPLEADO_H_

#include "Persona.h"
#include<string>


class Empleado: public Persona {//El modificador public hay que ponerlo delante de cada clase

	double sueldo;
	std::string empresa;

public:
	Empleado();
	Empleado(std::string nombre, std::string apellidos, float peso, float altura, double sueldo, std::string empresa);
	virtual ~Empleado();
};

#endif /* EMPLEADO_H_ */
