#ifndef PUNTO2D_H_
#define PUNTO2D_H_

class Punto2D {

	int x,y;

public:
	Punto2D(int=0,int=0);
	float distancia(const Punto2D &);
	void imprimir()const;
	Punto2D masCercano(Punto2D *, int);

	virtual ~Punto2D();
};

#endif /* PUNTO2D_H_ */
