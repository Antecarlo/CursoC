#ifndef VECTOR_H_
#define VECTOR_H_

namespace std {
namespace literals {

class Vector{

	int * numeros;
	int size;
	int cont;

public:
	Vector(int tama);

	void imprimir();

	bool add(int num);

	void set(int pos, int numero);

	virtual ~Vector();
};

} /* namespace literals */
} /* namespace std */

#endif /* VECTOR_H_ */
