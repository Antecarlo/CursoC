#include "Punto2D.h"

int main(){
	Punto2D nube[]={Punto2D(7,2),Punto2D(4,2),Punto2D(0,3),Punto2D(9,2)};
	int n=sizeof(nube)/sizeof(Punto2D);

	Punto2D punto(9,1);
	const Punto2D masCercano=punto.masCercano(nube, n);

	//punto.masCercano(nube,n).imprimir;
	masCercano.imprimir();

}
